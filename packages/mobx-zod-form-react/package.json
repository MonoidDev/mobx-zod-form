{
  "name": "@monoid-dev/mobx-zod-form-react",
  "version": "0.13.0",
  "type": "commonjs",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "src/",
    "lib/"
  ],
  "author": "Chenyu Wang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MonoidDev/mobx-zod-form"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.tsx --external react --format esm,cjs -d lib/ && tsc -p tsconfig.build.json",
    "lint": "tsc && eslint ./src --ext .ts,.tsx",
    "test": "vitest run"
  },
  "dependencies": {
    "@monoid-dev/mobx-zod-form": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "mobx": "^6.8.0",
    "react": "^18.2.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.6.2",
    "@types/react": "^18.2.22",
    "jsdom": "^21.1.2",
    "mobx-react": "^7.6.0",
    "@types/react-dom": "^18.2.7",
    "react-dom": "^18.2.0"
  }
}
